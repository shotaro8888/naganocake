<h1>会員一覧</h1>
 <table class="table">
      <thread>
        <th>
          <th style="width: 20%">会員ID</th>
          <th style="width: 20%">氏名</th>
          <th style="width: 40%">メールアドレス</th>
          <th style="width: 20%">ステータス</th>
        </th>
      </thread>
      <tbody>
        <% @customers.each do |customer| %>
        <tr>
          <td></td>
          <td><%= customer.id %></td>
          <td><%= link_to admin_customer_path(customer.id) do %><%= customer.last_name + customer.first_name %><% end %></td>
          <td><%= customer.email %></td>
          <td><%= customer.is_deleted ? '退会' : '有効' %></td>
        </tr>
        <% end %>
      </tbody>
    </table>