<h1>商品一覧</h1>
    <table class="table">
      <thead>
        <th>
          <th style="width: 10%">商品ID</th>
          <th style="width: 30%">商品名</th>
          <th style="width: 10%">税抜価格</th>
          <th style="width: 30%">ジャンル</th>
          <th style="width: 20%">ステータス</th>
        </th>
      </thead>
      <% @items.each do |item| %>
      <tbody>
        <tr>
          <td></td>
          <td><%= item.id %></td>
          <td>
            <%= link_to admin_item_path(item.id) do %>
              <%= item.name %>
            <% end %>
          </td>
          <td><%= item.price %></td>
          <td><%= item.genre.name %></td>
          <td><%= item.is_active_i18n %></td>
        </tr>
      </tbody>
      <% end %>
    </table>